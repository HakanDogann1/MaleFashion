
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">

@await Component.InvokeAsync("HomePageHeadViewComponent")
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Offcanvas Menu Begin -->
    @await Component.InvokeAsync("HomePageMenuViewComponent")
    <!-- Offcanvas Menu End -->
    <!-- Header Section Begin -->
    @await Component.InvokeAsync("HomePageHeaderSectionViewComponent")
    <!-- Header Section End -->
    <!-- Hero Section Begin -->
    @await Component.InvokeAsync("HomePageHeoSectionViewComponent")
    <!-- Hero Section End -->
    <!-- Banner Section Begin -->
    @await Component.InvokeAsync("HomePageBannerViewComponent")
    <!-- Banner Section End -->
    <!-- Product Section Begin -->
    @await Component.InvokeAsync("HomePageProductSectionViewComponent")
    <!-- Product Section End -->
    <!-- Categories Section Begin -->
    @await Component.InvokeAsync("HomePageCategoriesSectionViewComponent")
    <!-- Categories Section End -->
    <!-- Instagram Section Begin -->
    @await Component.InvokeAsync("HomePageInstagramSectionViewComponent")
    <!-- Instagram Section End -->
    <!-- Latest Blog Section Begin -->
    @await Component.InvokeAsync("HomePageBlogSectionViewComponent")
    <!-- Latest Blog Section End -->
    <!-- Footer Section Begin -->
    @await Component.InvokeAsync("HomePageFooterSectionViewComponent")
    <!-- Footer Section End -->
    <!-- Search Begin -->
    @await Component.InvokeAsync("HomePageSearchBeginViewComponent")
    <!-- Search End -->
    <!-- Js Plugins -->
    @await Component.InvokeAsync("HomePageScriptViewComponent")
</body>

</html>